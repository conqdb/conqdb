    "@tiptap/starter-kit": "^2.5.0",
    "@types/lodash.pick": "^4.4.9",
    "arctic": "^1.9.1",
    "bson-objectid": "^2.0.4",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.11",
    "discord.js": "^14.16.2",
    "embla-carousel-react": "^8.1.6",
    "graphql": "^16.8.1",
    "immer": "^10.1.1",
    "lodash.pick": "^4.4.0",
    "lucia": "^3.2.0",
    "mantine-form-zod-resolver": "^1.1.0",
    "nanoid": "^5.0.7",
    "next": "15.0.0-canary.159",
    "next-intl": "^3.17.0",
