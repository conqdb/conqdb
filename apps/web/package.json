    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.12",
    "@types/node": "^20.12.12",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "@types/uuid": "^10.0.0",
    "bump-payload": "^1.2.1",
    "dotenv": "^16.4.5",
